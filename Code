PP = 5.93    # purchase price (change this value)
CH = 6.00    # Cach given to teller by customer (change this value)

#Coin types
mydict = {'PENNY': .01,
'NICKEL': .05,
'DIME': .10,
'QUARTER': .25,
'HALF DOLLAR': .50,
'ONE': 1.00,
'TWO': 2.00,
'FIVE': 5.00,
'TEN': 10.00,
'TWENTY': 20.00,
'FIFTY': 50.00,
'ONE HUNDRED': 100.00}


if CH < PP:
    
    print("ERROR! Cash can't be less than purchase price!") #self explantory
    
elif CH==PP:
    
    print("ZERO! No change!") #self explantory
    
else:
    
    change = float("{0:.2f}".format(CH - PP))  # Here we must specify the number of float digits in order not to get crazy value
    
    
    #Initialize some variable that will be used in the loops below

    mychange = change 
    
    while mychange > 0:
        
        for i in mydict: # Loop over the values in the dict.
            
            last_temp = 1000  # random initilization value
            
            temp = float("{0:.2f}".format(mychange - mydict[i])) # Get the difference between the change and value in dict.
            
            if (temp < last_temp) and (temp >= 0): # find the minimum value 
                
                min_temp = temp
                
                last_temp =temp
                
                needed_dict = i   # This is the dict key that gives the minimum difference.
        
        coin = needed_dict   # This is the dict key that gives the minimum difference.
        
        mychange = float("{0:.2f}".format(mychange - mydict[coin])) # Update mychange for th next iteration.
        
        if mychange != 0.0: # This if condition is just to print the comma properly. There could be another way to do it.
            
            print(coin, end=", ") 
 
        elif mychange ==0.0:

            print(coin)
            
